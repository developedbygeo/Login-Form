[
  {
    "id": "firstName",
    "regex": "^[a-zA-Z]+$"
  },
  {
    "id": "lastName",
    "regex": "^[a-zA-Z]+$"
  },
  {
    "id": "email",
    "regex": "^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$"
  },
  {
    "id": "zip",
    "regex": "^[0-9]+$"
  },
  {
    "id": "password",
    "regex": "^(?=.*[0-9])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{7,15}$"
  },
  {
    "id": "passwordConfirm",
    "regex": "^(?=.*[0-9])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{7,15}$"
  }
]
